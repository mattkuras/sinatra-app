<h1>The Hikes</h1>

<a href="/hikes/new"><h3>Create Hike</h3></a>
<a href="/users/<%=current_user.username%>"><h3>My Hikes</h3></a>
<% Hike.all.each do |hike| %>
<a href="/hikes/<%=hike.id%>"> <h2> <%= hike.name %> </h2> </a>
<a href="/user/<%=hike.user.username%>"> <p> <%= hike.user.username %> </p> </a>
<h3> Difficulty: <%= hike.difficulty %> </h3>
<h3> Rating: <%= hike.rating %> </h3>
<%end%>
